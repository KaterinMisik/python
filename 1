def Sort(listy):
#for index in listy:
    l = len(listy)
    for i in range(0, l):
        #for j in range(0, l):
        for j in range(0, l-i-1):
            if (listy[j][-1] > listy[j+ 1][-1]):
                temp = listy[j]
                listy[j]= listy[j + 1]
                listy[j + 1]= temp
    return listy
print(Sort(listy))

def mergeSort(lista):
    if len(lista) > 1:
        mid = len(lista) // 2
        left = lista[:mid]
        right = lista[mid:]
 
        mergeSort(left)
        mergeSort(right)
 
        i = 0
        j = 0
        k = 0
       
        while i < len(left) and j < len(right):
            if left[i] < right[j]:
              lista[k] = left[i]
              i += 1
            else:
                lista[k] = right[j]
                j += 1
            k += 1
 
        while i < len(left):
            lista[k] = left[i]
            i += 1
            k += 1
 
        while j < len(right):
            lista[k]=right[j]
            j += 1
            k += 1
 
mergeSort(lista)
print("lista posortowana", lista)

